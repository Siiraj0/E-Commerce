            <div class="product-slider-active product-slider-padding dot-style-1">
              <% latestProducts.forEach(product => { %>
              <div class="product-wrap product-wrap-opacity">
                <div  class="product-img default-overlay mb-40">
                  <a href="/product/<%= product._id %>">
                    <img class="default-img" src="<%= product.image[0] %>" alt="<%= product.name %>">
                    <% if (product.image[1]) { %>
                    <img class="hover-img" src="<%= product.image[1] %>" alt="<%= product.name %>">
                    <% } %>
                  </a>
                
                </div>
                <div class="product-content">
                  <h3 class="title-blod"><a href="/product/<%= product._id %>"><%= product.name %></a></h3>
                  <div class="product-price">
                    <% if (product.offerPrice) { %>
                      <span class="new-price">₹<%= product.offerPrice %></span>
                      <span class="old-price">₹<%= product.price %></span>
                    <% } else { %>
                      <span class="new-price">₹<%= product.price %></span>
                    <% } %>
                  </div>
                </div>
              </div>
              <% }) %>
            </div>
