    <script>
        function password(e){
            const password= document.getElementById('password')
            if(e.classList.contains("ri-eye-off-fill")){
                e.classList.remove("ri-eye-off-fill");
                e.classList.add("ri-eye-line");
                
                password.type="text"
            }else{
                e.classList.remove("ri-eye-line");
                e.classList.add("ri-eye-off-fill")
                password.type = "password";
            }
            
        }
    </script>     

